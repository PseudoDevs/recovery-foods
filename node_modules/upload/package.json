    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "husky": "^4.2.3",
    "jest": "^25.2.6",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
